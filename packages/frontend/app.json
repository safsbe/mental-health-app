{
  "expo": {
    "name": "hello-world",
    "slug": "reagent-expo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/mulish/Mulish_400.ttf",
            "node_modules/@expo-google-fonts/mulish/Mulish_700.ttf",
            "node_modules/@expo-google-fonts/mulish/Mulish_800.ttf"
          ]
        }
      ]
    ]
  }
}
