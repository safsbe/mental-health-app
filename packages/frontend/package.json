{
  "name": "@safsbe/mental-health-app-frontend",
  "version": "0.0.1",
  "private": true,
  "main": "./app/index.js",
  "homepage": "/mental-health-app-demo",
  "scripts": {
    "postinstall": "shadow-cljs classpath && rm -r vendor/assets ; git clone https://github.com/safsbe/mental-health-app-assets vendor/assets && cp -r vendor/assets/* assets/",
    "prebuild:common": "shadow-cljs release app",
    "prebuild:android": "npm run prebuild:common && expo prebuild --platform=android",
    "build:android:apk": "cd android && ./gradlew assembleRelease && cd -",
    "build:web": "npm run prebuild:common && expo export --platform=web",
    "watch:common": "shadow-cljs watch app",
    "watch:web": "expo start --web",
    "watch:android:expo": "expo run:android",
    "dev-test-android-build": "react-native bundle --dev false --platform android --entry-file app/index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "dev-test-android-run": "./gradlew assembleDebug",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "@types/react": "~18.2.79",
    "autoprefixer": "^10.4.19",
    "expo-doctor": "^1.6.1",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "shadow-cljs": "^2.28.11"
  },
  "dependencies": {
    "@expo-google-fonts/mulish": "^0.2.3",
    "@expo/cli": "^0.18.13",
    "@expo/image-utils": "^0.5.1",
    "@expo/metro-runtime": "~3.2.1",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@ronradtke/react-native-markdown-display": "^8.0.0",
    "create-react-class": "^15.7.0",
    "expo": "^51.0.22",
    "expo-asset": "~10.0.6",
    "expo-av": "~14.0.6",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.6",
    "expo-image": "~1.12.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "^0.19.12"
  }
}
